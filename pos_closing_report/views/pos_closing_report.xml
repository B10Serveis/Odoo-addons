<?xml version="1.0" encoding="utf-8"?>
<openerp>
<template id="pos_closing_report">
    <t t-call="report.html_container">
        <t t-foreach="docs" t-as="o">
            <t t-call="report.external_layout">
                <div class="page">
                    <div>
                        <span>POS CLOSING REPORT</span> 
                    </div>
                    <div>
                        Terminal: <span t-field="docs.config_id.name"/>
						Des de: <span t-field="docs.date_ini" /> Fins a: <span t-field="docs.date_fi" />
                    </div>
					
                </div>
            </t>
        </t>
    </t>
</template>
</openerp>
