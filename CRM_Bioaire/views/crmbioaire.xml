                <field name="contact_name"/>
		<field name="phone"/>
		<field name="email_from"/>
		<field name="city"/>
		<field name="provincia"/>
		</group>
		<group string="Wharehouse Data">
		<field name="press_tipus"/>
                <field name="metres_nau"/>
		<field name="tipus_nau"/>
		<field name="tipus_sostre"/>
		<field name="ventilacio"/>
		<field name="alt_nau"/>
		</group>
		</group>
        </page>
	</xpath>
    </field>
</record>
</data>
</openerp>
